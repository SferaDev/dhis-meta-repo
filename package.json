{
  "name": "dhis-meta-repo",
  "description": "DHIS2 Metadata git repository builder",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "author": "Alexis Rico",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SferaDev/dhis-meta-repo.git"
  },
  "scripts": {
    "start": "NODE_ENV=development babel-watch src --extensions \".js,.jsx,.ts,.tsx\"",
    "clean": "rimraf build",
    "prebuild": "yarn clean",
    "build": "babel src --out-dir build --extensions \".js,.jsx,.ts,.tsx\"",
    "predist": "rimraf dist && yarn build",
    "dist": "ncc build build/ -m",
    "postdist": "cp dist/index.js $npm_package_name.js",
    "prettify": "prettier \"{.,src}/**/*.{js,jsx,ts,tsx,json,css}\" --write"
  },
  "dependencies": {
    "@babel/runtime": "7.16.3",
    "@eyeseetea/d2-api": "1.9.3",
    "commander": "8.3.0",
    "dotenv": "10.0.0",
    "fs-extra": "10.0.0",
    "lodash": "4.17.21",
    "log4js": "6.3.0",
    "nodegit": "0.27.0",
    "tmp": "0.2.1",
    "yargs": "17.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/node": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-typescript": "7.16.0",
    "@types/express": "4.17.13",
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.177",
    "@types/node": "16.11.7",
    "@types/node-schedule": "1.3.2",
    "@types/nodegit": "0.27.5",
    "@types/tmp": "0.2.2",
    "@types/yargs": "17.0.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "@zeit/ncc": "0.22.3",
    "babel-eslint": "10.1.0",
    "babel-watch": "7.6.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.4.1",
    "typescript": "4.5.2"
  }
}
