{
  "name": "dhis-meta-repo",
  "description": "DHIS2 Metadata git repository builder",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "author": "Alexis Rico",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SferaDev/dhis-meta-repo.git"
  },
  "scripts": {
    "start": "NODE_ENV=development babel-watch src --extensions \".js,.jsx,.ts,.tsx\"",
    "clean": "rimraf build",
    "prebuild": "yarn clean",
    "build": "babel src --out-dir build --extensions \".js,.jsx,.ts,.tsx\"",
    "predist": "rimraf dist && yarn build",
    "dist": "ncc build build/ -m",
    "postdist": "cp dist/index.js $npm_package_name.js",
    "prettify": "prettier \"{.,src}/**/*.{js,jsx,ts,tsx,json,css}\" --write"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "commander": "^5.0.0",
    "d2-api": "^0.2.0-beta.2",
    "dotenv": "^8.1.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "log4js": "^5.1.0",
    "nodegit": "^0.26.5",
    "tmp": "^0.1.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.5",
    "@types/node-schedule": "^1.2.4",
    "@types/nodegit": "^0.26.1",
    "@types/tmp": "^0.1.0",
    "@types/yargs": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "@zeit/ncc": "^0.20.5",
    "babel-eslint": "^10.0.3",
    "babel-watch": "^7.0.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  }
}
